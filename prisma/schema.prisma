// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Symbol {
  id        String   @id @default(cuid())
  name      String   @unique
  enabled   Boolean  @default(true)
  // Optional enrichment fields from Yahoo (sector, industry, exchange, type)
  sector    String?
  industry  String?
  exchange  String?
  type      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
